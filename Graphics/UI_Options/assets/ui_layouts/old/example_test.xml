            <image>gigolord_walk0/LF-WxWalk_01#[02].png</image>
          </element>                 
          <element name="text01" type="text" xpos="center" sizefont="18" color="yellow,255" >Navegador de prueba</element>                       
        </element>
    
        <element name="chrome-btn" type="button" selectablestatus="deactive" allocationtext="down" sizefont="18">        
