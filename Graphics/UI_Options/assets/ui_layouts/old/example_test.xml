      <element name="chromeall-btn" type="multioption" xpos="100" ypos="30">   
        
        <element name="chrome-btn" type="button" selectablestatus="active" allocationtext="down" sizefont="18">        
          <element name="animation01" type="animation" xpos="center" state="play" timedelay="35">
            <image>gigolord_walk0/LF-WxWalk_01#[02].png</image>
          </element>                 
          <element name="text01" type="text" xpos="center" sizefont="18" color="white,255" >Navegador de prueba</element>                       
        </element>
        
        <element name="chrome-btn" type="button" selectablestatus="preselect"  allocationtext="down" sizefont="18">        
