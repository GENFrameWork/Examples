<?xml version="1.0" encoding="UTF-8"?>
<layouts>
  <skin name="FLAT" drawmode="canvas" rasterfont="verdana18" vectorfont="Nunito-SemiBold.ttf" backgroundimg="background.jpg" > </skin>

  <color name="colortext">yellow</color>   
  <color name="colormenuline">red</color>   
  <color name="colormenu">black,50</color>
  

  <layout name="example">

