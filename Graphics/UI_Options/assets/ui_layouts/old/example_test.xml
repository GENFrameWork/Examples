            <image>gigolord_walk0/LF-WxWalk_01#[02].png</image>
          </element>                 
          <element name="text01" type="text" xpos="center" sizefont="18" color="white,255" >Navegador de prueba</element>                       
        </element>   
        
      </element>
          
      <element name="email-btn" type="button" xpos="340" ypos="30"  allocationtext="down" sizefont="18" visible="yes">       
        <element name="animation02" type="animation">
          <image>gmail-logo.png</image>
        </element>                 
