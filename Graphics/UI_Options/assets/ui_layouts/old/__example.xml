<?xml version="1.0" encoding="UTF-8"?>
<layouts>
  <skin name="" drawmode="canvas" rasterfont="verdana18" vectorfont="Inter-Black.ttf"> </skin>


  <layout name="startup">                 

  <element name="text01" type="text" xpos="100" ypos="100" margin="10,10,10,10" aling="none" sizefont="16" color="white,255" >Texto directo en el Layer ...</element>    
  
  </layout>                 


  <layout name="IN_ALL">                 
  
    <element name="menu" type="form"  xpos="300" ypos="480" width="500" height="80" color="10,10,10,150">
  
      <element name="chrome-btn" type="button"  xpos="20" ypos="30" aling="horcenter" allocationtext="down" sizefont="18">
        
        <element name="animation01" type="animation" aling="none" state="play" timedelay="35">
          <image>gigolord_walk0/LF-WxWalk_01#[02].png</image>
        </element>         
        
        <element name="text01" type="text" sizefont="18" color="white,255" >Navegador</element>               
        
      </element>
    
    
    
      <element name="email-btn" type="button" xpos="190" ypos="30" aling="horcenter" allocationtext="down" sizefont="18" visible="yes">
        
        <element name="animation01" type="animation" aling="none">
          <image>gmail-logo.png</image>
        </element>         
        
        <element name="text01" type="text" sizefont="18" color="white,255" >Email</element>               
        
      </element>



      <element name="youtube-btn" type="button" xpos="310" ypos="30" aling="horcenter" allocationtext="down" sizefont="18">
        
        <element name="animation01" type="animation" aling="none">
          <image>youtube-logo.png</image>
        </element>         
        
        <element name="text01" type="text" sizefont="18" color="white,255" >Youtube</element>               
        
        
      </element>
      
    </element>    
       
  </layout>

</layouts> 


